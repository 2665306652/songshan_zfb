<view class="grace-news-list data-v-27555a20"><block a:if="{{itemList}}"><block class="data-v-27555a20"><block a:for="{{itemList}}" a:for-item="item" a:for-index="index" a:key="index"><view data-event-opts="{{[['tap',[['artList',['$0'],[[['itemList','',index,'id']]]]]]]}}" class="itemNews data-v-27555a20" onTap="__e"><view class="item_picture data-v-27555a20"><image class="picture data-v-27555a20" src="{{item.img}}" mode="scaleToFill"></image></view><view class="item_details data-v-27555a20"><view class="details_identity data-v-27555a20"><view class="details_name data-v-27555a20">{{item.name}}</view><view class="details_service data-v-27555a20">{{item.level}}</view><view class="details_age data-v-27555a20">{{item.newAge}}</view></view><view class="details_manifestation data-v-27555a20"><view class="manifestation_grade data-v-27555a20">{{item.score+"分"}}</view><view class="manifestation_serveNumber data-v-27555a20">{{item.servicetimes+"次服务"}}</view><view class="manifestation_comment data-v-27555a20">{{"("+item.commentnumber+"条点评)"}}</view></view><view class="item_tagline data-v-27555a20">{{item.comments}}</view></view></view></block></block></block><block a:if="{{itemList.length<=0}}"><block class="data-v-27555a20">暂时没有数据</block></block></view>